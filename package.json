{
  "name": "photos.reupen.uk",
  "private": true,
  "description": "photos.reupen.uk website",
  "version": "1.0.0",
  "author": "Reupen Shah",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/reupen/photos.reupen.uk"
  },
  "scripts": {
    "astro": "astro",
    "check": "astro check --minimumFailingSeverity hint",
    "clean": "shx rm -rf dist node_modules/.astro",
    "build": "astro build",
    "postbuild": "python scripts/clean_up_dist.py",
    "dev": "astro dev",
    "format:check": "prettier -c .",
    "lint": "eslint --max-warnings 0 .",
    "preview": "astro preview",
    "start": "astro dev",
    "pretest": "npm run build",
    "test": "python scripts/run_in_playwright_container.py \"npm ci && npm exec --no -- playwright test\"",
    "test:report": "playwright show-report test/e2e/output/html",
    "pretest:update": "npm run build",
    "test:update": "python scripts/run_in_playwright_container.py \"npm ci && npm exec --no -- playwright test --update-snapshots\""
  },
  "dependencies": {
    "@astrojs/sitemap": "^3.5.1",
    "@fontsource-variable/noto-sans": "^5.2.7",
    "@fortawesome/fontawesome-free": "^7.0.0",
    "astro": "^5.13.5",
    "date-fns": "^4.1.0",
    "sass": "^1.89.2",
    "swiped-events": "^1.2.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.34.0",
    "@playform/compress": "^0.2.0",
    "@playwright/test": "^1.53.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^22.14.0",
    "astro-purgecss": "^5.2.2",
    "clsx": "^2.1.1",
    "cssnano": "^7.1.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "exiftool-vendored": "^30.3.0",
    "globals": "^16.3.0",
    "html-to-text": "^9.0.5",
    "node-addon-api": "^8.4.0",
    "node-gyp": "^11.2.0",
    "postcss-custom-media": "^11.0.6",
    "postcss-preset-env": "^10.3.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "purgecss": "^7.0.2",
    "sharp": "^0.34.1",
    "shx": "^0.4.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "user-agent-data-types": "^0.4.2"
  },
  "overrides": {
    "sharp": "$sharp"
  }
}
